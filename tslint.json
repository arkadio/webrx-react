{
  "extends": "tslint-config-unional",
  "rules": {
    "member-ordering": [ true, "static-before-instance", "variables-before-functions" ],
    "no-unused-variable": [ true, "react" ],
    "typedef": [ false ],
    "variable-name": [ true, "ban-keywords" ],
    "interface-name": [ false ]
  }
}
